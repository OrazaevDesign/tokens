{
  "primary": {
    "rest": {
      "bg": {
        "$type": "color",
        "$value": "{roles.primary.primary}"
      },
      "border": {
        "$type": "color",
        "$value": "{roles.primary.primary}"
      },
      "lowest": {
        "$type": "color",
        "$value": "{roles.primary.on-primary-container}"
      },
      "hightest": {
        "$type": "color",
        "$value": "{roles.primary.primary-container}"
      }
    },
    "default": {
      "color": {
        "$type": "color",
        "$value": "{roles.primary.on-primary}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{0}"
      }
    },
    "hovered": {
      "color": {
        "$type": "color",
        "$value": "{roles.primary.on-primary}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{20}"
      }
    },
    "focused": {
      "color": {
        "$type": "color",
        "$value": "{roles.primary.on-primary}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{100}"
      }
    },
    "disabled": {
      "color": {
        "$type": "color",
        "$value": "{roles.primary.on-primary}"
      },
      "state": {
        "$type": "number",
        "$value": "{80}"
      }
    }
  },
  "clear": {
    "rest": {
      "bg": {
        "$type": "color",
        "$value": "{palettes.transparent}"
      },
      "border": {
        "$type": "color",
        "$value": "{palettes.transparent}"
      },
      "lowest": {
        "$type": "color",
        "$value": "{roles.primary.on-primary-container}"
      },
      "hightest": {
        "$type": "color",
        "$value": "{palettes.transparent}"
      }
    },
    "default": {
      "color": {
        "$type": "color",
        "$value": "{roles.surface.on-surface-variant}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{0}"
      }
    },
    "hovered": {
      "color": {
        "$type": "color",
        "$value": "{roles.surface.on-surface}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{20}"
      }
    },
    "focused": {
      "color": {
        "$type": "color",
        "$value": "{roles.surface.on-surface}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{100}"
      }
    },
    "disabled": {
      "color": {
        "$type": "color",
        "$value": "{roles.outline.outline variant}"
      },
      "state-opacity": {
        "$type": "number",
        "$value": "{80}"
      }
    }
  }
}